{
  "plugins": [
    "tbfed-pagefooter",
    "anchor-navigation-ex",
    "mermaid-gb3",
    "insert-logo",
    "chapter-fold",
    "advanced-emoji",
    "github",
    "splitter",
    "alerts",
    "popup",
    "prism",
    "-highlight",
    "-livereload",
    "hide-element"
  ],
  "title": "C# 多线程与异步 - 痴者工良",
  "author": "痴者工良",
  "description": "这是一本关于 C# 多线程与异步 的书，作者 痴者工良",
  "language": "zh-hans",
  "links": {
    "sidebar": {
      "痴者工良的博客": "https://www.whuanle.cn"
    }
  },
  "pluginsConfig": {
    "tbfed-pagefooter": {
      "copyright": "Copyright &copy 痴者工良 2022",
      "modify_label": "文档最后更新时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "insert-logo": {
      "url": "/images/logo.jpg",
      "style": "background: none; max-height: 50px; min-height: 50px"
    },
    "github": {
      "url": "https://github.com/whuanle/csharp_thread"
    },
    "prism": {
      "lang": {
        "flow": "typescript",
        "shell": "bash"
      },
      "ignore": [
        "mermaid",
        "eval-js"
      ],
      "css": [
        "prismjs/themes/prism.css"
      ],
      "js": [
        "prismjs/prism.js",
        "prismjs/components.js",
        "prismjs/components/prism-go.min.js",
        "prismjs/components/prism-yaml.min.js",
        "prismjs/components/prism-bash.min.js",
        "prismjs/components/prism-shell-session.min.js"
      ]
    },
    "anchor-navigation-ex": {
      "showLevel": false, 
      "showGoTop": false 
    },
    "hide-element": {
      "elements": [".gitbook-link"]
  }
  }
}